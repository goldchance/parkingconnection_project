<% title "Places" %>

<table>
  <tr>
    <th>Href</th>
    <th>Location</th>
    <th>Address</th>
    <th>Price</th>
    <th>Urlimage</th>
    <th>Desc</th>
    <th>Source</th>
  </tr>
  <% for place in @places %>
    <tr>
      <td><%= place.href %></td>
      <td><%= place.location %></td>
      <td><%= place.address %></td>
      <td><%= place.price %></td>
      <td><%= place.urlimage %></td>
      <td><%= place.desc %></td>
      <td><%= place.source_id %></td>
      <td><%= link_to "Show", place %></td>
      <td><%= link_to "Edit", edit_place_path(place) %></td>
      <td><%= link_to "Destroy", place, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Place", new_place_path %></p>
