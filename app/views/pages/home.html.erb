<script>
   $(document).ready(function() {
     
        
      $('#findbtn3').click(function(){
        $('#centralpark_link').click();
        $('#parkwhiz_link').click();
        $('#parkingpanda_link').click();
      });
      $('#findbtn').click(function(){
        var wherebox, city, state
        wherebox = $('#wherebox').val();
        city = wherebox.split(",")[0].replace(/^\s+|\s+$/g, '').replace(" ","-")
        
        state = wherebox.split(",")[1].replace(/^\s+|\s+$/g, '')
        from = $('#from').val();
        to = $('#to').val();
        time_start = $('#Items').val();
        time_end = $('#Items2').val();
        if ($('#centralpark').is(':checked')) {
          var city_short
          if ( city.toLowerCase() == "new-york" )
          { city_short = "nyc" }
          else
          { city_short = city.replace(" ","").replace("-","") }
          url = "http://" + city_short + ".centralparking.com/parking-near/" + city + "-" + state + "-USA.html"
         $('#centralpark_link').click(function(){
                   window.open(url, 'blank1');
                });


          //window.open(url, 'window name', 'window settings');
       
       }
        if ($('#parkwhiz').is(':checked')) {
          url = "http://www.parkwhiz.com/p/"+city+"-parking/map/?&start_date="+ from
                   +"&start_time=" + time_start +"&end_date="+ to +"&end_time="+time_end 
        $('#parkwhiz_link').click(function(){
            window.open(url, 'blank2');
      });

        $('#parkwhiz_link').click();
                   // window.open(url, 'window name', 'window settings');
        }
        if ($('#parkingpanda').is(':checked')) {
          url=  "https://www.parkingpanda.com/Search/?location="+ city + "%2c"+state+"%2c+USA&start=" + from 
          +"#location="+city +"%2C%20"+state+"%2C%20USA&start="+from+"&end="+to+"&monthly=false&daily=true"
          //window.open(url, 'window name', 'window settings');
                $('#parkingpanda_link').click(function(){
            window.open(url, 'blank3');
          
          });

        $('#parkingpanda_link').click();
        }
      });
   });
</script>
